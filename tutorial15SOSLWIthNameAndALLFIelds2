List<list<SObject>> customerList=[FIND 'ABC*'
IN ALL  Fields
RETURNING APEX_customer__c(Id,APEX_Customer__c.Name,APEX_Customer__c.Apex_Customer_Description__c)];

System.debug('the result is '+customerList);
